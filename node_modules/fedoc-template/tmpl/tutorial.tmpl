<section>
<header>
    <?js if (children.length > 0) { ?>
    <ul><?js
        var self = this;
        children.forEach(function(t) { ?>
        <li><?js= self.tutoriallink(t.name) ?></li>
    <?js }); ?></ul>
    <?js } ?>

    <h2><?js= header ?></h2>
</header>

<article>
	<?js

		var pathPrefix = 'tutorials/';
        var src = pathPrefix + filename.replace('tutorial-', '');
    ?>
    <iframe id="exampleFrame" width="500" height="700" frameborder="0" src=<?js= src ?> onload="autoResize()"></iframe>
</article>
<script>
function autoResize() {
    var ef = document.getElementById('exampleFrame'),
    doc;

    if(ef.contentDocument) {
        doc = ef.contentDocument;
    } else {
        doc = ef.contentWindow.document;
    }
    ef.width = document.getElementById('main').clientWidth - 40;
    ef.height = doc.body.clientHeight + 40;
}
</script>

</section>